package net.technikumwien.bic4b18_01.spread;

import java.io.File;
import java.io.IOException;
import java.util.logging.Level;
import java.util.logging.Logger;

/**
 *
 * @author Florian
 */
public class daemonApplication {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        File trunk = (new File(daemonApplication.class.getProtectionDomain().getCodeSource().getLocation().getPath())).getParentFile().getParentFile().getParentFile();
        System.out.println(trunk.getPath());
        try {
            Runtime.getRuntime().exec("cmd /c start "+trunk.getPath()+"\\spread\\start_spread_win.bat");
        } catch (IOException ex) {
            Logger.getLogger(daemonApplication.class.getName()).log(Level.SEVERE, null, ex);
        }
    }
    
}
