package net.technikumwien.bic4b18_01.spread.local;

import java.io.File;
import java.io.IOException;
import java.util.logging.Level;
import java.util.logging.Logger;

/**
 *
 * @author Florian
 */
public class DaemonApplication {

    private static final Logger logger = Logger.getLogger(DaemonApplication.class.getName());

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        File trunk = (new File(DaemonApplication.class.getProtectionDomain().getCodeSource().getLocation().getPath())).getParentFile().getParentFile().getParentFile();
        System.out.println(trunk.getPath());
        //ConfigSetup.create();
        try {
            Runtime.getRuntime().exec("cmd /c start " + trunk.getPath() + "\\spread\\start_spread_win.bat");
        } catch (IOException ex) {
            logger.log(Level.SEVERE, null, ex);
        }
    }

}
